function bmiCalculator (Weight,height) {
  var bmi = weight/Math.pow(height,2);
  
  if (bmi < 18.5){
  return "Your Bmi " + bmi + ", so you are overweight";
  }
  else if (bmi >= 18.5 && bmi <= 24.9){
  return "Your Bmi " + bmi + ", so you have normal weight";
  }
  else if(bmi > 24.9){
  return "Your Bmi " + bmi + ", so you are underweight";
  }
  else {
  return bmi;
  }
 }
